#include<iostream>
#include <bits/stdc++.h>
using namespace std;
 
string PalindromeCheck(string S)
{
    
    for (int i = 0; i<S.length()/2; i++) {
 
        
        if (S[i]!= S[S.length()-i-1]) 
        {
          cout<<"Hi"";    
            return "No";
        }
    }
    
    return "Yes";
}
 

int main()
{
    
    string S = "RACECAR";
    cout<<PalindromeCheck(S);
 
    return 0;
}
